            <Button fx:id="buttonCriar" layoutX="330.0" layoutY="116.0" mnemonicParsing="false" onAction="#acaoCriarCandidatos" prefHeight="120.0" prefWidth="620.0" text="Criar Candidatos" />
            <Button fx:id="buttonSair" layoutX="330.0" layoutY="397.0" mnemonicParsing="false" onAction="#acaoSair" prefHeight="120.0" prefWidth="620.0" text="Sair" />
